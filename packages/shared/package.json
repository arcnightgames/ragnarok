{
  "name": "@arcnight/shared",
  "version": "0.1.0",
  "description": "Arcnight shared",
  "license": "AGPL-3.0",
  "author": "Arcnight Inc.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/shared.js"
    }
  },
  "main": "dist/shared.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix",
    "pretty": "prettier --check $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)",
    "pretty:fix": "prettier --write $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)"
  },
  "dependencies": {
    "@arcnight/crypto": "workspace:packages/crypto",
    "@arcnight/utils": "workspace:packages/utils",
    "jsbi": "^4.3.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@arcnight/colors": "workspace:packages/colors",
    "@arcnight/eslint-config-arcnight": "workspace:packages/eslint-config-arcnight",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1"
  }
}
