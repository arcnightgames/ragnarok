{
  "name": "@arcnight/storybook",
  "version": "0.1.0",
  "description": "Arcnight Design system",
  "license": "AGPL-3.0",
  "author": "Arcnight Inc.",
  "type": "module",
  "scripts": {
    "build": "storybook build --docs --output-dir dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "pretty": "prettier --check $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)",
    "pretty:fix": "prettier --write $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)",
    "storybook": "storybook dev --docs --no-open -p 6006",
    "storybook-raw": "storybook dev --no-open -p 6006"
  },
  "dependencies": {
    "@arcnight/components": "workspace:packages/components",
    "@arcnight/hooks": "workspace:packages/hooks",
    "@arcnight/shared": "workspace:packages/shared",
    "@arcnight/styles": "workspace:packages/styles",
    "@arcnight/utils": "workspace:packages/utils",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@arcnight/eslint-config-arcnight": "workspace:packages/eslint-config-arcnight",
    "@storybook/addon-docs": "^8.1.3",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-storysource": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/builder-vite": "^8.1.3",
    "@storybook/manager-api": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-vite": "^8.1.3",
    "@storybook/theming": "^8.1.3",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "storybook": "^8.1.3",
    "vite": "^5.2.8"
  }
}
